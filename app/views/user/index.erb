<div class='flex-container-vertical'>

  <div class='page_section'>
    <table>
      <tr>
        <th>Username</th>
        <th>Email</th>
      </tr>
      <% User.all.each do |user| %>
        <tr>
          <td><a href = '/user/<%=user.slug%>'><%=user.username %></a></td>
          <td><%=user.email %></td>
        </tr>
      <% end %>
    </table>
  </div>
</div>
